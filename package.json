{
	"name": "cron2yaml",
	"displayName": "cron2yaml",
	"description": "Convert cron entries to yaml",
	"version": "0.9.0",
  "publisher": "klaudioz",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:cron2yaml.convert"
	],
	"main": "./out/cron2yaml.js",
  "contributes": {
        "menus": {
            "editor/context": [
                {
                    "command": "cron2yaml.convert",
                    "group": "9_cutcopypaste@500"
                }
            ]
        },
        "commands": [
            {
                "command": "cron2yaml.convert",
                "title": "Convert cron entries to yaml"
            }
        ]
    },
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
  "repository": {
        "type": "git",
        "url": "https://github.com/Klaudioz/cron2yaml.git"
  }
}
